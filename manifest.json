{
  "manifest_version": 2,
  "name": "Busca Posiciones",
  "version": "1.0",
  "description": "Este plugin muestra la posicion de un dominio en concreto",
  "icons": {
     "16": "icon16.png",
     "48": "icon48.png",
     "128": "icon128.png"
            },
  "browser_action": {
    "default_title": "Busca Posiciones",
    "default_icon": "icon.png",
    "default_popup": "popup.html" 
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "file:///*/*",
    "https://*.googleapis.com/"
  ],
  "content_security_policy": 
    "script-src 'self' https://ajax.googleapis.com; object-src 'self'"


  
}